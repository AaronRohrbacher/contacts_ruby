<h2>Enter your information</h2>
<% @list.each do |names| %>
  <h3><a href='/output/<%= names.id%>'><%= names.first_name %></h3></a>
<% end %>
<form action="/" method="post">
  <div class="form-group">
    <label for="field1">First Name</label>
    <input id="1" name="first_name" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="field1">Last name</label>
    <input id="2" name="last_name" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="field1">Job title</label>
    <input id="3" name="job_title" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="field1">Company</label>
    <input id="4" name="company" class="form-control" type="text">
  </div>
  <div class="form-group">
    <label for="field1">Contact_type</label>
    <input id="5" name="contact_type" class="form-control" type="text">
  </div>
  <button type="submit" class="btn btn-primary">Go</button>
</form>
